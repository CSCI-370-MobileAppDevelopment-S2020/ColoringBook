<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.introtoandroid.coloringbook2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SportsImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".NatureImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".FoodImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".LandscapeImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".AnimalsImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".ArchitectureImageSelect" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".SecondActivityMain" android:parentActivityName=".MainActivity"></activity>

    </application>

</manifest>